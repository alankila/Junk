#!/system/bin/sh

# 1st: analog offsets 0 .. 255
# 2nd: analog gain 0 .. 15
# 3rd: 0C reference value 0 .. 255

ACCELEROMETER_OFFSET="-0.08 -1.22 -0.22"
ACCELEROMETER_SCALE="1 0.98 0.95"
#ACCELEROMETER_OFFSET="0 0 0"
#ACCELEROMETER_SCALE="1 1 1"
MAGNETIC_GAIN="5 4 6"
TEMPERATURE_ZERO="112"

exec akmd.free $ACCELEROMETER_OFFSET $ACCELEROMETER_SCALE $MAGNETIC_GAIN $TEMPERATURE_ZERO
